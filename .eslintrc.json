{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "standard",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "space-before-function-paren": 0,
    "no-useless-constructor": 0,
    "comma-dangle": 0,
    "n/no-path-concat": 0,
    "prefer-promise-reject-errors": 0
  }
}
